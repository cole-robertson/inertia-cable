{
  "name": "@inertia-cable/react",
  "version": "0.2.2",
  "description": "React hooks for InertiaCable - ActionCable integration for Inertia.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@inertiajs/react": ">=1.0.0",
    "@rails/actioncable": ">=7.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@inertiajs/react": "^2.0.0",
    "@rails/actioncable": "^7.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/rails__actioncable": "^6.1.0",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "inertia",
    "inertiajs",
    "actioncable",
    "rails",
    "react",
    "websocket",
    "realtime"
  ],
  "license": "MIT"
}
